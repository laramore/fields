{
    "name": "laramore/fields",
    "description": "Define all default fields used in Laramore",
    "keywords": ["ORM", "Laravel", "Model", "Migration", "Automatic", "Auto", "Fields", "Field", "Meta", "Table", "Database", "Validations"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": ">=5.0",
        "nesbot/carbon": ">=1.0.0",
        "symfony/http-kernel": ">=4.0.0",
        "laramore/base": "^1.0",
        "laramore/types": "^1.0"
    },
    "authors": [
        {
            "name": "Nastuzzi Samy",
            "email": "samy@nastuzzi.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laramore\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpcs src",
            "./vendor/bin/phpmd src text phpmd.xml"
        ]
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laramore\\Providers\\FieldsProvider",
                "Laramore\\Providers\\ConstraintsProvider",
                "Laramore\\Providers\\ProxiesProvider"
            ],
            "aliases": {
                "Constraints": "Laramore\\Facades\\Constraints",
                "Proxies": "Laramore\\Facades\\Proxies"
            }
        }
    }
}
